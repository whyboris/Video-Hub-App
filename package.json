{
  "name": "video-hub-app",
  "version": "1.9.6",
  "description": "Video Hub App - browse, search, preview your videos",
  "homepage": "http://www.videohubapp.com",
  "author": {
    "name": "Boris Yakubchik",
    "email": "yboris@yahoo.com"
  },
  "contributors": [
    {
      "name": "Cal Martin",
      "url": "https://github.com/cal2195"
    }
  ],
  "keywords": [
    "video"
  ],
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "debug": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serveDebug",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serveDebug": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --debug",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.0.17",
    "@ffprobe-installer/ffprobe": "1.0.9",
    "@ngx-translate/core": "11.0.1",
    "ngx-virtual-scroller": "2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.8",
    "@angular/animations": "7.2.4",
    "@angular/cli": "7.2.4",
    "@angular/common": "7.2.4",
    "@angular/compiler": "7.2.4",
    "@angular/compiler-cli": "7.2.14",
    "@angular/core": "7.2.4",
    "@angular/forms": "7.2.4",
    "@angular/platform-browser": "7.2.4",
    "@angular/platform-browser-dynamic": "7.2.4",
    "@angular/router": "7.2.4",
    "@types/node": "10.12.24",
    "codelyzer": "4.5.0",
    "core-js": "2.6.4",
    "electron": "4.2.0",
    "electron-builder": "20.38.5",
    "electron-reload": "1.4.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.4.0",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.2.4",
    "wait-on": "3.2.0",
    "zone.js": "0.8.29"
  }
}
