{
  "name": "video-hub-app-3",
  "version": "3.0.0",
  "description": "Video Hub App 3 - browse, search, preview your videos",
  "homepage": "http://www.videohubapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/whyboris/Video-Hub-App.git"
  },
  "author": {
    "name": "Boris Yakubchik",
    "email": "yboris@yahoo.com"
  },
  "contributors": [
    {
      "name": "Cal Martin",
      "url": "https://github.com/cal2195"
    }
  ],
  "keywords": [
    "video"
  ],
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "buildsize": "sh ./bin/buildSizeCheck.sh",
    "electron": "npm run build:prod && electron-builder build && npm run buildsize",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "ng": "ng",
    "ng:serve": "ng serve",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.0.20",
    "@ffprobe-installer/ffprobe": "1.1.0",
    "@ngx-translate/core": "13.0.0",
    "async": "3.2.0",
    "chokidar": "3.4.3",
    "electron-window-state": "5.0.3",
    "fdir": "4.1.0",
    "fuse.js": "6.4.3",
    "natural-orderby": "2.0.3",
    "ngx-virtual-scroller": "4.0.3",
    "path": "0.12.7",
    "trash": "7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.2",
    "@angular/animations": "11.0.2",
    "@angular/cdk": "11.0.1",
    "@angular/cli": "11.0.2",
    "@angular/common": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/compiler-cli": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/material": "11.0.1",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "@types/node": "12.12.6",
    "codelyzer": "5.2.2",
    "electron": "8.5.5",
    "electron-builder": "22.9.1",
    "electron-reload": "1.5.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.3",
    "stylelint": "13.8.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.5",
    "wait-on": "5.2.0",
    "zone.js": "0.11.3"
  },
  "browserslist": [
    "chrome 83"
  ]
}
